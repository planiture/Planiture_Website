@using Planiture_Website.Models;
@model IEnumerable<ApplicationUser>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<div class="">
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>
        </thead>
        <tbody>
           @foreach(ApplicationUser user in Model)
           {
               <tr>@user.Id</tr>
               <tr>@user.FirstName</tr>
               <tr>@user.LastName</tr>
           }

        </tbody>
    </table>
</div>